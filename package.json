{
  "name": "braintree-web-debugger",
  "private": true,
  "version": "0.3.0",
  "description": "A browser extension for debugging the Braintree web sdk",
  "scripts": {
    "pretty": "prettier --write .",
    "lint": "eslint --ext ts .",
    "posttest": "npm run pretty && npm run lint",
    "test": "jest",
    "build": "npm run build:chrome && npm run build:firefox",
    "build:firefox": "NODE_ENV=production BROWSER=FIREFOX webpack",
    "build:chrome": "NODE_ENV=production BROWSER=GOOGLE_CHROME webpack",
    "start": "webpack serve"
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/preset-env": "^7.13.5",
    "@babel/register": "^7.13.0",
    "@types/chrome": "0.0.133",
    "@types/jest": "^26.0.20",
    "@types/shortid": "0.0.29",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "babel-jest": "^26.6.3",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.1.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-notifier-webpack-plugin": "^3.0.0",
    "fork-ts-checker-webpack-plugin": "^6.1.0",
    "fs-extra": "^9.1.0",
    "html-loader": "^2.1.1",
    "html-webpack-plugin": "^5.2.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.9",
    "prettier": "^2.2.1",
    "shortid": "^2.2.16",
    "string-replace-loader": "^3.0.1",
    "ts-jest": "^26.5.2",
    "ts-loader": "^8.0.17",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.2.2",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "dependencies": {
    "@braintree/asset-loader": "^0.4.4",
    "framebus": "^5.1.2"
  }
}

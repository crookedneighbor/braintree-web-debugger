{
  "name": "Braintree Web SDK Debugger",
  "short_name": "braintree-web-debugger",
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "js": ["main.bundle.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "web_accessible_resources": ["fake-braintree.bundle.js", "braintree-logo.jpg"],
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "manifest_version": 2
}
